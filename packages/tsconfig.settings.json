{
    "compilerOptions": {
        "composite": true,
        "target": "ES5",
        "module": "UMD",
        "moduleResolution": "Node",
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "importHelpers": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "DOM",
            "ES2015"
        ],
        "types": []
    }
}